{
  "name": "@perses-dev/dashboards",
  "version": "0.53.0-rc.2",
  "description": "The dashboards feature in Perses",
  "license": "Apache-2.0",
  "homepage": "https://github.com/perses/perses/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perses/perses.git"
  },
  "bugs": {
    "url": "https://github.com/perses/perses/issues"
  },
  "module": "dist/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist/",
    "build": "concurrently \"npm:build:*\"",
    "build:cjs": "swc ./src -d dist/cjs --strip-leading-paths --config-file ../.cjs.swcrc",
    "build:esm": "swc ./src -d dist --strip-leading-paths --config-file ../.swcrc",
    "build:types": "tsc --project tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "start": "concurrently -P \"npm:build:* -- {*}\" -- --watch",
    "test": "cross-env TZ=UTC jest",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx"
  },
  "dependencies": {
    "@perses-dev/components": "0.53.0-rc.2",
    "@perses-dev/core": "0.53.0-rc.0",
    "@perses-dev/plugin-system": "0.53.0-rc.2",
    "@types/react-grid-layout": "^2.1.0",
    "date-fns": "^4.1.0",
    "immer": "^11.1.3",
    "mdi-material-ui": "^7.9.2",
    "react-grid-layout": "^2.2.2",
    "react-hook-form": "^7.46.1",
    "react-intersection-observer": "^10.0.2",
    "use-immer": "^0.11.0",
    "use-query-params": "^2.2.1",
    "use-resize-observer": "^9.0.0",
    "yaml": "^2.7.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "history": "^5.3.0",
    "intersection-observer": "^0.12.2"
  },
  "peerDependencies": {
    "@mui/material": "^6.1.10",
    "@tanstack/react-query": "^4.39.1",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "files": [
    "dist"
  ]
}